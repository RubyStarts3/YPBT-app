/ div id="tag-bar-loading"
- @whole_info.time_tags.each do |tag|
  .tag-point {
    style = "left:calc(#{tag.start_time_percentage*100}% - 8px);\
             background-color:rgb(255,#{tag.like_color},#{tag.like_color})"
    onclick = "event.stopPropagation(); seekTo(#{tag.start_time});"
    id= "#{tag.time_tag_id}"
    data-container = "body"
  }
